<link rel="stylesheet" href="<%= assetPath('css/signUp.css') %>">

<div class="form-container">
    <h1>Change Password</h1>

    <div class="signUpForm">
        <form action="/reset_passwords/changePassword/<%= Reset.accessToken %>" method="POST">
            <input type="password" class="headers" name="password" placeholder="Password" required>
            <input type="password" class="headers" name="confirm_password" placeholder="Confirm Password" required>
            <input type="submit" class="button changePassword" value="Change Password">

        </form>
    </div>
</div>
